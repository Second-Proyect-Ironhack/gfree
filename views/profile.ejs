<div id="profile">
  <div id="profile-content">
  <nav id="nav">
      <div class="nav-wrapper">
        <a href="#" class="brand-logo">Profile</a>
        <ul class="nav-mobile">
          <li><a class="back" href="/map">Go back</a></li>
        </ul>
      </div>
    </nav>

<div class="background-image">
</div>

<section class="section1">
  <div class="img-profile">
<img src="<%= user.picture.pic_path %>"><br>
</div>

  <h1><%= user.username %></h1>
      <!-- <a href="#" class="brand-logo">Gfree</a> -->
      <ul>
        <li><a href="/edit">Edit profile</a></li>|
        <li><a href="/<%= user._id %>/delete/">Delete your account</a></li>
      </ul>

</section>


<div class="card">
   <div class="card-image waves-effect waves-block waves-light">
     <img class="activator" src="/images/card3.jpg">
   </div>
   <div class="card-content">
     <span class="card-title activator grey-text text-darken-4">Favorite places<i class="material-icons right">more_vert</i></span>
     <p><a href="#">Show</a></p>
   </div>
   <div class="card-reveal">
     <span class="card-title grey-text text-darken-4">Favorites<i class="material-icons right">close</i></span>
                   <% places.forEach(place =>{%>
                     <% if(place.favorite.length > 0){%>
                     <%for(var i = 0; i< place.favorite.length; i++){%>
                       <%if(place.favorite[i].toString() === user._id.toString()){%>
                           <p><i class="material-icons">star_border</i><%= place.name%></p>
                       <%}%>
                     <%}}%>
                   <%})%>
   </div>
 </div>

